datasource DS {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = env("BINARY_TARGET")
}

// Models

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  authId String @unique
  customerId String @unique
}
